[
  {
    "kernelSource": {
      "name": "Huawei_Hi6250_AllInOne",
      "repo": "https://github.com/Abdelhay-Ali/android_kernel_huawei_hi6250_KernelSU",
      "branch": "main",
      "device": "merge",
      "defconfig": "Abdelhay_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://github.com/Haseo97/aarch64-elf-gcc",
        "branch": "master",
        "name": "gcc",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/AndroJr7/arm-linux-gnueabi",
        "branch": "master",
        "name": "gcc-32",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "gcc/bin/aarch64-elf-gcc",
      "externalCommand": {
        "CROSS_COMPILE": "gcc/bin/aarch64-elf-",
        "CROSS_COMPILE_ARM32": "gcc-32/bin/arm-linux-gnueabi-",
        "LD": "gcc/bin/aarch64-elf-ld",
        "AR": "gcc/bin/aarch64-elf-ar",
        "NM": "gcc/bin/aarch64-elf-nm",
        "OBJCOPY": "gcc/bin/aarch64-elf-objcopy",
        "OBJDUMP": "gcc/bin/aarch64-elf-objdump",
        "READELF": "gcc/bin/aarch64-elf-readelf",
        "OBJSIZE": "gcc/bin/aarch64-elf-size",
        "STRIP": "gcc/bin/aarch64-elf-strip",
        "LDGOLD": "gcc/bin/aarch64-elf-ld.gold",
        "LLVM_AR": "proton-clang/bin/llvm-ar",
        "LLVM_DIS": "proton-clang/bin/llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": false,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "thyme"
    }
  }
]
