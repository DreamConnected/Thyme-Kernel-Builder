[
  {
    "kernelSource": {
      "name": "Huawei_Hi6250_AllInOne",
      "repo": "https://github.com/Abdelhay-Ali/android_kernel_huawei_hi6250_KernelSU",
      "branch": "main",
      "device": "merge",
      "defconfig": "Abdelhay_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-gnu-9.3",
        "branch": "lineage-21.0",
        "name": "gcc",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/Ubuntu-Touch-GalaxyS/gcc-9.3-arm-linux-gnueabihf",
        "branch": "gcc-9.3",
        "name": "gcc-32",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "gcc/bin/aarch64-linux-gcc",
      "externalCommand": {
        "CROSS_COMPILE": "gcc/bin/aarch64-linux-",
        "CROSS_COMPILE_ARM32": "gcc-32/bin/arm-linux-",
        "LD": "gcc/bin/aarch64-linux-ld",
        "AR": "gcc/bin/aarch64-linux-ar",
        "NM": "gcc/bin/aarch64-linux-nm",
        "OBJCOPY": "gcc/bin/aarch64-linux-objcopy",
        "OBJDUMP": "gcc/bin/aarch64-linux-objdump",
        "READELF": "gcc/bin/aarch64-linux-readelf",
        "OBJSIZE": "gcc/bin/aarch64-linux-size",
        "STRIP": "gcc/bin/aarch64-linux-strip",
        "LDGOLD": "gcc/bin/aarch64-linux-ld.gold",
        "LLVM_AR": "proton-clang/bin/llvm-ar",
        "LLVM_DIS": "proton-clang/bin/llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": false,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "thyme"
    }
  }
]
