[
  {
    "kernelSource": {
      "name": "Xiaomi_SM8250_AllInOne",
      "repo": "https://github.com/LineageOS/android_kernel_xiaomi_sm8250",
      "branch": "lineage-20",
      "device": "munch",
      "defconfig": "gki_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://gitlab.com/ThankYouMario/android_prebuilts_clang-standalone",
        "branch": "17",
        "name": "clang",
        "binPath": ["bin"]
      }
    ],
    "ccache": false,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "LD": "clang/bin/ld.lld",
        "AR": "clang/bin/llvm-ar",
        "NM": "clang/bin/llvm-nm",
        "OBJCOPY": "clang/bin/llvm-objcopy",
        "OBJDUMP": "clang/bin/llvm-objdump",
        "READELF": "clang/bin/llvm-readelf",
        "OBJSIZE": "clang/bin/llvm-size",
        "STRIP": "clang/bin/llvm-strip",
        "LLVM_AR": "clang/bin/llvm-ar",
        "LLVM_DIS": "clang/bin/llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": false,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "thyme"
    }
  }
]
