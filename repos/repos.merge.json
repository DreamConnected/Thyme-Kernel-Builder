[
    {
        "kernelSource": {
            "name": "Xiaomi-cepheus-all-in-one",
            "repo": "https://github.com/crdroidandroid/android_kernel_xiaomi_cepheus/",
            "branch": "12.0",
            "device": "cepheus",
            "defconfig": "cepheus_defconfig"
        },
        "withKernelSU": true,
        "disableinet": true,
        "withLXCPatch": true,
        "withDockerLXC": true,
        "toolchains": [
            {
                "repo": "https://gitlab.com/ThankYouMario/android_prebuilts_clang-standalone/",
                "branch": "16",
                "name": "clang",
                "binPath": [
                    "bin"
                ]
            },
            {
                "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-gnu-6.4.1",
                "branch": "lineage-18.1",
                "name": "gcc-64",
                "binPath": [
                    "bin"
                ]
            },
            {
                "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_arm_arm-linux-androideabi-4.9/",
                "branch": "lineage-17.1",
                "name": "gcc-32",
                "binPath": [
                    "bin"
                ]
            }
        ],
        "ccache": true,
        "params": {
            "ARCH": "arm64",
            "CC": "gcc-64/bin/aarch64-linux-gnu-gcc",
            "externalCommand": {
                "CROSS_COMPILE": "gcc-64/bin/aarch64-linux-gnu-",
                "CROSS_COMPILE_ARM32": "gcc-32/bin/arm-linux-androideabi-",
                "LD": "gcc-64/bin/aarch64-linux-gnu-ld",
                "AR": "gcc-64/bin/aarch64-linux-gnu-ar",
                "NM": "gcc-64/bin/aarch64-linux-gnu-nm",
                "OBJCOPY": "gcc-64/bin/aarch64-linux-gnu-objcopy",
                "OBJDUMP": "gcc-64/bin/aarch64-linux-gnu-objdump",
                "READELF": "gcc-64/bin/aarch64-linux-gnu-readelf",
                "OBJSIZE": "gcc-64/bin/aarch64-linux-gnu-size",
                "STRIP": "gcc-64/bin/aarch64-linux-gnu-strip",
                "LDGOLD": "gcc-64/bin/aarch64-linux-gnu-ld.gold",
                "LLVM_AR": "gcc-64/bin/aarch64-linux-gnu-ar",
                "LLVM_DIS": "gcc-64/bin/aarch64-linux-gnu-dis"
            }
        },
        "AnyKernel3": {
            "use": true,
            "release": true,
            "repo": "https://github.com/easterNday/AnyKernel3/",
            "branch": "thyme"
        }
    }
]
