[
  {
    "kernelSource": {
      "name": "Oneplus_SDM845_AllInOne",
      "repo": "https://github.com/kimocoder/android_kernel_oneplus6",
      "branch": "development",
      "device": "fajita",
      "defconfig": "sdm845_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-gnu-6.4.1",
        "branch": "lineage-18.1",
        "name": "gcc",
        "binPath": ["bin"]
      },
      {
        "repo": "https://github.com/innfinite4evr/android-prebuilts-gcc-linux-x86-arm-arm-eabi-7.2",
        "branch": "master",
        "name": "gcc-32",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "gcc/bin/aarch64-linux-gnu-gcc",
      "externalCommand": {
        "CROSS_COMPILE": "gcc/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "gcc-32/bin/arm-eabi-",
        "LD": "gcc/bin/aarch64-linux-gnu-ld",
        "AR": "gcc/bin/aarch64-linux-gnu-ar",
        "NM": "gcc/bin/aarch64-linux-gnu-nm",
        "OBJCOPY": "gcc/bin/aarch64-linux-gnu-objcopy",
        "OBJDUMP": "gcc/bin/aarch64-linux-gnu-objdump",
        "READELF": "gcc/bin/aarch64-linux-gnu-readelf",
        "OBJSIZE": "gcc/bin/aarch64-linux-gnu-size",
        "STRIP": "gcc/bin/aarch64-linux-gnu-strip",
        "LDGOLD": "gcc/bin/aarch64-linux-gnu-ld.gold"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": false,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "thyme"
    }
  }
]

